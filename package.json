    "build:analyze": "npx cross-env@7.0.3 ANALYZE=true next build",
    "cypress:run": "npx cypress@8.3.0 run --record",
    "cypress:run:firefox": "npx cypress@8.3.0 run --record  --browser firefox",
    "cypress:run:chrome": "npx cypress@8.3.0 run --record --browser chrome",
    "cypress:run-local": "npx cypress@8.3.0 run",
    "cypress:run-local:firefox": "npx cypress@8.3.0 run --browser firefox",
    "cypress:run-local:chrome": "npx cypress@8.3.0 run --browser chrome",
    "cypress:open": "npx cypress@8.3.0 open",
    "dekk-backend:clean": "npx rimraf@3.0.2 node_modules/@dekk-app/dekk-backend/src/colonies/prisma/client/query-engine-darwin node_modules/@dekk-app/dekk-backend/src/colonies/prisma/client/query-engine-debian-openssl-1.1.x",
    "dev": "next",
    "graphql:setup": "node prepare/graphql-setup.js",
    "graphql:types": "graphql-codegen",
    "lint": "yarn tsc:noEmit && yarn xo && yarn stylelint && next lint",
    "mock:sw": "npx cpy-cli prepare/mockServiceWorker.js public",
    "mock:sw:clean": "npx rimraf@3.0.2 'public/mockServiceWorker.js'",
    "ncu": "npx npm-check-updates@11.8.3 -u --dep='prod,dev'",
    "ncu:minor": "yarn ncu -t minor",
    "ncu:patch": "yarn ncu -t patch",
    "npmrc:setup": "node prepare/npmrc.js",
    "prepare": "husky install",
    "prettier": "prettier 'src/**/*.{ts,tsx}'",
    "prettier:write": "yarn prettier --write",
    "release": "npx standard-version@9.3.1",
    "spj": "npx sort-package-json@1.50.0 package.json",
    "start": "next start",
    "stylelint": "stylelint 'src/**/*.{ts,tsx}'",
    "pretest": "yarn mock:sw",
    "test": "npx start-server-and-test@1.12.6 start http://localhost:3000 cypress:run",
    "posttest": "yarn mock:sw:clean",
    "pretest:local": "yarn mock:sw",
    "test:local": "npx start-server-and-test@1.12.6 start http://localhost:3000 cypress:run-local",
    "posttest:local": "yarn mock:sw:clean",
    "tsc:noEmit": "tsc --noEmit",
    "unimported": "npx unimported@1.12.0",
    "xo": "xo",
    "xo:fix": "yarn xo --fix"
